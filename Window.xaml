<Window x:Class="WiazanieKompilacji.EditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Produkt" Height="220" Width="350">
    <Grid>
        <StackPanel>
            <TextBox x:Name="txtSymbol" Margin="5" />
            <TextBox x:Name="txtNazwa" Margin="5" />
            <TextBox x:Name="txtLiczbaSztuk" Margin="5" />
            <TextBox x:Name="txtMagazyn" Margin="5" />
            <Button Content="PotwierdŸ" Click="btnPotwierdz_Click" Margin="5" />
        </StackPanel>
    </Grid>
</Window>
